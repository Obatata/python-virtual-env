CONDA : 
-------
# afficher les virtualenv(s)
conda env list

# création d'un virtualenv intitulé "env" avec python3.7
conda create -n env python=3.7

# activation du virtualenv intitulé "env"
conda activate env

# desactivation du virtualen "env"
deactivate


VIRTUALENV:
-----------
# installer avec pip
pip install virtualenv

# création d'un virtualenv intitulé "env"
virtualenv -p python3 env

# activation de virtualenv "env" 
source env/bin/activate

# desactivation du virtualenv "env"
deactivate
